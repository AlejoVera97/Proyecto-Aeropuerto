
@model EntidadesCompartidas.Aeropuertos

@{
    ViewBag.Title = "BAJA AEROPUERTO";
}

@ViewBag.Mensaje


<h3>Esta Seguro que desea eliminar el siguiente Articulo?</h3>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(Model=>Model.IDaeropuerto)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IDaeropuerto);
        </dd>

        <dt>
            @Html.DisplayNameFor(model=> model.Nombre)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Direccion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ImpuestoLlegada)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ImpuestoParitda)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Volver", "Principal")
        </div>
    }
</div>

